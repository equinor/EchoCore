{
    "name": "@equinor/echo-core",
    "version": "0.6.8",
    "description": "Echo Core - Core functionality for the echo.equinor.com",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "source": "./src/index.ts",
    "scripts": {
        "build": "rollup -c",
        "start": "rollup -c -w",
        "echo-publish": "npm publish --access public",
        "echo-publish-beta": "npm publish --access public --tag beta",
        "echo-publish-next": "npm publish --access public --tag next",
        "echo-test": "jest",
        "patch": "npm version patch",
        "major": "npm version major",
        "minor": "npm version minor",
        "test-wo-install": "jest --watchAll",
        "test": "npm ci & jest --watchAll",
        "test-coverage": "npm ci & jest --coverage --watchAll",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
        "build-docs": "npx typedoc --out docs src/index.ts",
        "echo-update-all": "echo-update -a"
    },
    "author": "Echo Core",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/equinor/EchoCore",
        "directory": "packages/echo-core"
    },
    "peerDependencies": {
        "@equinor/echo-base": "^0.6.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.3.0"
    },
    "dependencies": {
        "@azure/msal-browser": "^2.22.0",
        "@dbeining/react-atom": "^4.1.21",
        "@microsoft/applicationinsights-web": "^2.7.3",
        "sha1": "^1.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@equinor/echo-base": "^0.6.3",
        "@microsoft/microsoft-graph-types": "^2.14.0",
        "@rollup/plugin-babel": "^5.3.1",
        "@rollup/plugin-commonjs": "^21.0.2",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@testing-library/react-hooks": "^7.0.2",
        "@types/jest": "^27.4.0",
        "@types/react": "^17.0.39",
        "@types/react-router-dom": "^5.3.3",
        "@types/sha1": "^1.1.3",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.4.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.3.0",
        "react-test-renderer": "^17.0.2",
        "rollup": "^2.68.0",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-tslib-resolve-id": "0.0.0",
        "rollup-plugin-typescript-paths": "^1.3.0",
        "rollup-plugin-typescript2": "^0.31.2",
        "typedoc": "^0.22.12",
        "typescript": "^4.5.5"
    },
    "files": [
        "dist/"
    ]
